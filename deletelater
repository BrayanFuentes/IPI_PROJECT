cout << "Playing Solitaire Prime!!!" << endl;
			int totalPrime = 0;
			int sum = 0;
			while (main.CardsLeft() < 53)
			{
				Card card = main.deal();
				sum += card.getValue();
				if (isPrime(sum))
				{
					card.showCard();
					cout << ", Prime:" << sum << endl;
					totalPrime += 1;
					sum = 0;
				}
				else

				{
					card.showCard();
					cout << ", ";
				}
			}


return storage[top];
	top++;
